<%for(var attribute in Cookbook.Models.Recipe.attributes){%>
	<%if(attribute == 'id' || attribute == 'created_at' || attribute == 'updated_at') continue;%>
	<td class='<%= attribute %>'>
		<input type="text" value="<%= this[attribute]%>" name="<%= attribute%>"/>
	</td>
<%}%>
<td class='created_at'>
    <%= this.attr('created_at') %>
</td>
<td class='updated_at'>
    <%= this.attr('updated_at') %>
</td>
<td>
	<input type='submit' value='Update' class='update'/>
	<a href='javascript://' class='cancel'>cancel</a>
</td>