<h2>subjects</h2>
<table cellspacing='0px'>
	<thead>
	<tr>
		<% for(var attr in Intrepica.Models.Subject.attributes){%>
			<% if(attr == 'id') continue;%>
			<th><%= attr%> </th>	 
		<%}%>
		<th>Options</th>
	</tr>
	</thead>
	<tbody>
		<%= $.View('//intrepica/views/subject/list',{subjects: subjects})%>
	</tbody>
</table>
<h2>New subject</h2>
<form>
	<% for(var attr in Intrepica.Models.Subject.attributes){ %>
		<% if(attr == 'id') continue;%>
		<div>
			<label><%= attr %> </label><br />
			<input type="text" value="" name="<%= attr %>"/>
		</div>
	<%}%>
	<input type='submit' value='Create'/>
</form>